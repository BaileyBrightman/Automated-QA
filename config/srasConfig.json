{
    "unpermitted": [
        "wav"
    ],
    "ignore": [
      "html",
      "css"
    ],
    "rules": {
        "js": {
            "maxSize": 1048576
        },
        "images": {
            "png": {
                "maxSize": 1048576,
                "allowed": true,
                "require": false,
                "powerOfTwo": false,
                "maxWidth": 2048,
                "maxHeight": 2048
            },
            "jpeg": {
                "maxSize": 1048576,
                "allowed": true,
                "require": false,
                "powerOfTwo": false,
                "maxWidth": 2048,
                "maxHeight": 2048
            }
        },
        "audio": {
            "mp3": {
                "maxSize": -1,
                "allowed": true,
                "require": true,
                "maxChannels": 1,
                "sampleRate": -1,
                "duration": -1
            },
            "ogg": {
                "maxSize": -1,
                "allowed": true,
                "require": false,
                "maxChannels": 1,
                "sampleRate": -1,
                "duration": -1
            }
        }
    }
}
